
public class sessao04exerciciofixacao {

	public static void main(String[] args) {
		String product1 = "Computer";
		String product2 = "Office Desk";
		
		int age = 30;
		int code = 5290;
		char gender = 'F';
		
		double price1 = 2100.0;
		double price2 = 650.50;
		double measure = 53.234567;
		
		System.out.println("Product:");
		System.out.printf("Computer, which price is $d", price1);
		System.out.printf("Office desk, which price is $d", price2);
		
		System.out.printf("Record: $d years old, code %d and gender: %s", age, code, gender);
		

	}

}
